{
  "name": "cpic_kit",
  "version": "1.0.0",
  "description": "CPIC KGrid Kit",
  "main": " ",
  "bin": {
    "cpic": "cli-client/cli-client.js"
  },
  "scripts": {
    "postinstall": "node scripts/clean.js && node scripts/kgridInstall.js",
    "clean": "node scripts/clean.js",
    "start": "scripty",
    "stop": "scripty",
    "test:postman": "newman run tests/cpic_kit.postman_collection.json -e tests/kit_test.postman_environment.json --reporters  cli,junit --reporter-junit-export test-results",
    "test:cli": "jest",
    "test:web": "testcafe chrome web/test/webdemotest.js",
    "test": "npm run test:postman && npm run test:cli",
    "package": "scripty",
    "cpic": "node cli-client/cli-client.js"
  },
  "author": "KGrid Team",
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.4.11",
    "axios": "^0.18.0",
    "commander": "^2.17.1",
    "csvtojson": "^2.0.8",
    "download": "^7.1.0",
    "file-exists": "^5.0.1",
    "fs": "0.0.1-security",
    "fs-extra": "^7.0.0",
    "http-server": "^0.11.1",
    "jest": "^23.6.0",
    "jsonpath": "^1.0.0",
    "newman": "^4.1.0",
    "request": "^2.88.0",
    "scripty": "^1.7.2",
    "shelljs": "^0.8.2",
    "testcafe": "^0.22.0",
    "unzip": "^0.1.11"
  },
  "scripty": {
    "path": "scripts",
    "windowsPath": "scriptswin",
    "silent": true
  },
  "githubAssets": {
    "library": {
      "url": "https://api.github.com/repos/kgrid/kgrid-library",
      "destination": "library"
    },
    "activator": {
      "url": "https://api.github.com/repos/kgrid/kgrid-activator",
      "destination": "activator"
    },
    "cpic-kos": {
      "url": "https://api.github.com/repos/kgrid-objects/cpic-objects",
      "destination": "dist"
    }
  }
}
