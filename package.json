{
  "name": "cpic_kit",
  "version": "1.1.1",
  "description": "CPIC KGrid Kit",
  "main": " ",
  "bin": {
    "cpic": "cli-client/cli-client.js"
  },
  "scripts": {
    "postinstall": "node scripts/clean.js && node scripts/kgridInstall.js",
    "clean": "node scripts/clean.js",
    "start": "scripty",
    "stop": "scripty",
    "test:postman": "newman run tests/cpic_kit.postman_collection.json -e tests/kit_test.postman_environment.json --reporters  cli,junit --reporter-junit-export test-results",
    "test:cli": "jest",
    "test:web": "testcafe chrome:headless web/test/webdemotest.js",
    "test": "npm run test:postman && npm run test:cli && npm run test:web ",
    "package": "scripty",
    "cpic": "node cli-client/cli-client.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "author": "KGrid Team",
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.4.11",
    "axios": "^0.18.0",
    "commander": "^2.17.1",
    "csvtojson": "^2.0.8",
    "download": "^7.1.0",
    "file-exists": "^5.0.1",
    "fs": "0.0.1-security",
    "fs-extra": "^7.0.0",
    "http-server": "^0.11.1",
    "jest": "^23.6.0",
    "json2csv": "^4.2.1",
    "jsonpath": "^1.0.0",
    "newman": "^4.1.0",
    "prettyjson": "^1.2.1",
    "promise-throttle": "^1.0.0",
    "request": "^2.88.0",
    "scripty": "^1.7.2",
    "shelljs": "^0.8.2",
    "unzip": "^0.1.11"
  },
  "devDependencies": {
    "testcafe": "^0.22.0",
    "testcafe-reporter-xunit": "^2.1.0",
    "vuepress": "^0.14.4"
  },
  "scripty": {
    "path": "scripts",
    "windowsPath": "scriptswin",
    "silent": true
  },
  "githubAssets": {
    "library": {
      "url": "https://api.github.com/repos/kgrid/kgrid-library",
      "destination": "library",
      "tag_name": "1.0.9"
    },
    "activator": {
      "url": "https://api.github.com/repos/kgrid/kgrid-activator",
      "destination": "activator",
      "tag_name": "1.0.3"
    },
    "cpic-kos": {
      "url": "https://api.github.com/repos/kgrid-objects/cpic-collection",
      "destination": "dist",
      "tag_name": "1.5.1"
    }
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  }
}
